<div class="container">
    <h1>Welcome to Twitter Clone</h1>
    <% if current_user %>
        <%= link_to "Sign Out", destroy_user_session_path(current_user), method: :get  %>
    <% else %>
        <%= link_to "Sign In", new_user_session_path %>
        <br><br>
        <%= link_to "Sign Up", new_user_registration_path %>
    <% end %>
</div>
